a=input("enter a boy name")
b=input("enter a girl name")
a1=list(a)
b1=list(b)
for i in range(len(a)-1):
    for j in range(len(b)-1):
        if a1[i]==b1[j]:
           a1[i]='2'
           b1[j]='2'
    count=0
if i in a1:
    if (i!='2'):
     count=count+1
    if(i!='2'):
     count=count+1
ans=list("FLAMES")
ind=0
while(len(ans)!=1):
    ind=(ind+(count-1))%len(ans)
    ans.pop(ind)
print(ans)
    
